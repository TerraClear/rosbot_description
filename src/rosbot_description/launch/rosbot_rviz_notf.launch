<?xml version="1.0"?>
<launch>

  <include file="$(find rosbot_description)/launch/rosbot_notf.launch"></include>

  <param name="robot_description" command="$(find xacro)/xacro.py '$(find rosbot_description)/urdf/rosbot_nolaser.xacro'"/>

  <!-- Show in Rviz   -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find rosbot_description)/rviz/rosbot.rviz" required="true" output="screen"/>

  <!-- <node name="tfrebroadcaster" pkg="rosbot_description" output="screen" type="tf_rebroadcaster.py"/> -->

</launch>
